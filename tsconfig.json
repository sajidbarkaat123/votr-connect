{
  "files": [],
  "references": [
    {
      "path": "./tsconfig.app.json"
    },
    {
      "path": "./tsconfig.node.json"
    }
  ],
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "types": [
      "cypress"
    ],
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
